import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number:");
        int num=sc.nextInt();
        int evennum=0;
        int oddnum=0;
        while(num!=0){
            
            int digit=num%10;
            if(digit%2==0){
                evennum++;
            }
            else{
                oddnum++;
            }
            num=num/10;
            
        }
        System.out.println(evennum);
        System.out.println(oddnum);
         
     }
    
