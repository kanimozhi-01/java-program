class Main {
    public static void main(String[] args) {
    Scanner scan=new Scanner(System.in);   
    int n=scan.nextInt();
    int sum=0;
    int x=n;
    while(n>0){
        int d=n%10;
        sum=sum+(d*d*d);
        n=n/10;
    }
    if(x==sum){
        System.out.println("Armstrong");
    }
    else{
        System.out.println("not a Armstrong");
    }

    
    }
}
